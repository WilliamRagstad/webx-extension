{
  "name": "webx",
  "publisher": "webx",
  "displayName": "WebX",
  "description": "WebX language support and syntax highlighting",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/webx-net/webx-extension/issues"
  },
  "author": {
    "name": "WebX Organization",
    "url": "https://web-x.net/"
  },
  "repository": {
    "type": "git",
    "directory": "webx-extension",
    "url": "https://github.com/webx-net/webx-extension"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "webx",
      "aliases": ["WebX", "webx"],
      "extensions": [".webx",".wx"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "webx",
      "scopeName": "source.webx",
      "path": "./syntaxes/webx.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "webx",
        "path": "./snippets/webx.json"
      }
    ]
  }
}
